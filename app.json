{
  "expo": {
    "name": "MusicApp",
    "slug": "MusicApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/guitar.png",
    "notification" : {
      "icon" : "./assets/guitar.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "blue",
          "mode": "production",
          "icon": "./assets/guitar.png",
          "sound" : ["./assets/monk.mp3"]
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.musiclogtracker123.ios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nick_wylie.music_tracker"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
