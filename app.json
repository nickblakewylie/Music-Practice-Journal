{
  "expo": {
    "name": "MusicJournal",
    "slug": "MusicJournal",
    "version": "2.1.3",
    "orientation": "portrait",
    "icon": "./assets/music_icon.png",
    "notification" : {
      "icon" : "./assets/music_icon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "blue",
          "mode": "production",
          "icon": "./assets/music_icon.png",
          "sound" : ["./assets/monk.mp3"]
        }
      ]
    ],
    "splash": {
      "image": "./assets/guitar_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E8DCB8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.musiclogtracker123.ios",
      "infoPlist":{
        "NSCameraUsageDescription": "This app needs camera access to take videos during your practice sessions",
        "NSMicrophoneUsageDescription": "This app needs microphone access to record the audio during your practice sessions"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-5263616863180217~4145689603"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nick_wylie.music_tracker",
      "config" : {
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
